{
    "title": "Blog",
    "description": "学习的一些知识与心得",
    "author": "UnisLab-ic-group",
    "output.name": "site",
    "language": "zh-hans",
    "gitbook": "3.2.3",
    "root": ".",
    
    "plugins": [
        "-sharing",
        "katex",
        "theme-default",
        "expandable-chapters",
        "chapter-fold",
        "splitter",
        "copy-code-button",
        "-lunr", 
        "-search", 
        "search-pro",
        "prism", 
        "-highlight",
        "hide-element",
        "alerts",
        "fontsettings",
        "tbfed-pagefooter"

        
    ],
    "pluginsConfig": {
        "fontsettings": {
            "size": 12
        },
        "theme-default": {
            "showLevel": false
        },
        "prism": {
            "css": [
                "prismjs/themes/prism-tomorrow.css"
            ]
        },
        "tbfed-pagefooter": {
            "copyright": "Copyright © Priself.zz@gmail.com 2023",
            "modify_label": "该文章修订时间：",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        }
    }
}