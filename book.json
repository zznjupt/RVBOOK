{
    "title": "Blog",
    "description": "学习的一些知识与心得",
    "author": "UnisLab-ic-group",
    "output.name": "site",
    "language": "zh-hans",
    "gitbook": "3.2.3",
    "root": ".",
    
    "plugins": [
        "-sharing",
        "katex",
        "theme-default",
        "expandable-chapters",
        "chapter-fold",
        "splitter",
        "copy-code-button",
        "-lunr", 
        "-search", 
        "search-pro",
        "prism", 
        "-highlight",
        "hide-element",
        "alerts",
        "fontsettings",
        "signature"
        
    ],
    "pluginsConfig": {
        "fontsettings": {
            "size": 12
        },
        "theme-default": {
            "showLevel": false
        },
        "prism": {
            "css": [
                "prismjs/themes/prism-tomorrow.css"
            ]
        },
        "signature": {
            "autotimestamp":{
                 "color":"gray",
                 "timeStampFormat": "MM/DD/YYYY HH:mm:ss"
            },
            "autocopyright":{
                "color":"gray",
                "owner":"<a href='http://qfshare.com' target='_blank'>Priself</a>",
                "center":true
            },
            "signature":{
                "key": "Provide by Priself "
            }
        }
    }
}