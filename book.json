{
    "title": "RVBOOK",
    "description": "记录 RISCV 学习的一些知识与心得",
    "author": "UnisLab-ic-group",
    "output.name": "site",
    "language": "zh-hans",
    "gitbook": "3.2.3",
    "root": ".",
    
    "plugins": [
        "-sharing",

        "katex",
        "theme-comscore",
        "expandable-chapters",
        "chapter-fold",
        "splitter",
        "copy-code-button",
        "-lunr", 
        "-search", 
        "search-pro",
        "prism", 
        "-highlight",
        "hide-element",
        "theme-fexa",
        "heading-anchors"
        
    ],
    "variables": {
        "themeFexa":{
            "nav":[
                {
                    "url":"https://github.com/zznjupt/RVBOOK",
                    "target":"_blank",
                    "name": "本书代码仓库"
                }
            ]
        }
    },
    "pluginsConfig": {
        "theme-default": {
            "showLevel": false
        },
        "prism": {
            "css": [
                "prismjs/themes/prism-tomorrow.css"
            ]
        },
        "pluginsConfig": {
            "hide-element": {
                "elements": [".gitbook-link"]
            }
        },
        "theme-fexa":{
            "search-placeholder":"输入关键字搜索",
            "logo":"./logo.png",
            "favicon": "./favicon.ico"
        }
    }
}