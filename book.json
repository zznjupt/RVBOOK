{
    "plugins": [
        "katex",
        "theme-comscore",
        "chapter-fold",
        "splitter",
        "copy-code-button",
        "-lunr", 
        "-search", 
        "search-pro",
        "prism", 
        "-highlight",
        "hide-element",
        "theme-fexa"
    ],
    "variables": {
        "themeFexa":{
            "nav":[
                {
                    "url":"https://github.com/zznjupt/RVBOOK",
                    "target":"_blank",
                    "name": "本书代码仓库"
                }
            ]
        }
    },
    "pluginsConfig": {
        "theme-default": {
            "showLevel": true
        },
        "prism": {
            "css": [
                "prismjs/themes/prism-tomorrow.css"
            ]
        },
        "pluginsConfig": {
            "hide-element": {
                "elements": [".gitbook-link"]
            }
        },
        "theme-fexa":{
            "search-placeholder":"输入关键字搜索",
            "logo":"./logo.png",
            "favicon": "./favicon.ico"
        }
    }
}