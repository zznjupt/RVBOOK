{
    "title": "Blog",
    "description": "学习的一些知识与心得",
    "author": "UnisLab-ic-group",
    "output.name": "site",
    "language": "zh-hans",
    "gitbook": "3.2.3",
    "root": ".",
    
    "plugins": [
        "-sharing",
        "katex",
        "theme-default",
        "expandable-chapters",
        "chapter-fold",
        "splitter",
        "copy-code-button",
        "-lunr", 
        "-search", 
        "search-pro",
        "prism", 
        "-highlight",
        "hide-element",
        "alerts"
        
    ],
    "variables": {

    },
    "pluginsConfig": {
        "fontSettings": {
            "size": 15
        },
        "prism": {
            "css": [
                "prismjs/themes/prism-tomorrow.css"
            ]
        },
        "pluginsConfig": {
            "hide-element": {
                "elements": [".gitbook-link"]
            }
        }
    }
}